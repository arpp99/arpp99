<script type="text/javascript">
    var guruSignUpAndLogin = {
        ERR_EMAIL_EMPTY : "Harap masukan email",
        ERR_PASSWORD_EMPTY: "Harap masukkan kata sandi",
        ERR_EMAIL_FORMAT: "Harap masukan email yang valid",
        ERR_EMAIL_EXISTS: "Email already exist",
        ERR_EMAIL_NOT_EXISTS: "Email tidak ditemukan",
        GENERATE_NEW_NONCE: 0
    };

    function loginSetCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays*24*60*60*1000));
        var expires = "expires="+d.toUTCString();
        document.cookie = cname + "=" + cvalue + "; " + expires;
    }

    function loginGetCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for(var i=0; i<ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') c = c.substring(1);
            if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
        }
        return "";
    }

    if (jQuery) {
            jQuery(document).ready(function() {
            var url_hash = window.location.hash;
            if (url_hash === '#sr' && loginGetCookie('pgregwelcome') === "" && guruApp && guruApp.userSession && guruApp.userSession.user && guruApp.userSession.user.username && guruApp.userSession.user.username !== 'anon.') {
                var guru_user_confirmation_success = true;
                var guru_user_email = " ( " + guruApp.userSession.user.username + " ) ";
                var guru_user_message_1 = "Akun anda";

                var guru_user_message_2 = "berhasil dibuat";
                jQuery("#modal-signup-after-confirmation .account-created-msg").text(guru_user_message_1 + guru_user_email + guru_user_message_2);

                //show the success dialog
                jQuery("#modal-signup-after-confirmation").modal('show');
                jQuery("#modal-signup-after-confirmation img").trigger("appear");
                loginSetCookie("pgregwelcome", "1", 1);
            }
        });
    }
</script>



